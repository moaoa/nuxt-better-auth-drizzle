    "@twilio/voice-sdk": "^2.18.0",
    "@unovis/ts": "^1.5.1",
    "@unovis/vue": "^1.5.1",
    "@vee-validate/zod": "^4.15.0",
    "@vueuse/core": "^12.7.0",
    "@vueuse/nuxt": "^12.7.0",
    "better-auth": "^1.3.9",
    "better-sqlite3": "^11.10.0",
    "bullmq": "^5.58.5",
    "bullmq-otel": "^1.0.1",
    "canvas-confetti": "^1.9.3",
    "clsx": "^2.1.1",
