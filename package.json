{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^18.0.0 || ^20.0.0"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "tsx ./db/migrate.ts && nuxt preview ",
    "lint": " eslint . --fix",
    "start": "nuxt start",
    "postinstall": "nuxt prepare",
    "db:generate": "drizzle-kit generate --config ./config/drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config ./config/drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config ./config/drizzle.config.ts",
    "db:push": "drizzle-kit push --config ./config/drizzle.config.ts",
    "db:reset": "tsx --env-file=.env ./db/reset.ts && npm run db:push",
    "db:drop": "npx dbmate drop",
    "redis:flush": "tsx --env-file=.env ./scripts/flush-redis.ts",
    "db:seed": "tsx --env-file=.env ./db/seeds/index.ts",
    "db:seed:credits": "tsx --env-file=.env ./db/seeds/add-credits-to-first-user.ts",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "dependencies": {
